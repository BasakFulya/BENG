
@model IEnumerable<blogging.Models.TAG>
@{ 

}
<div class="sidebox widget">
    <h3 class="widget-title section-title">Tags</h3>
    <ul class="tag-list">
        @foreach (blogging.Models.TAG tag in Model)
        {

            <li><a href="/Tag/Index/@tag.TagID">@tag.TagName (@tag.ARTICLEs.Count)</a></li>

        }





    </ul>
    <!-- /.tag-list -->

</div>
